      <researchPrerequisite Inherit="false" />
      <researchPrerequisites>
        <li>PrecisionRifling</li>
        <li>CE_AdvancedNV</li>
      </researchPrerequisites>
      <skillRequirements>
        <Crafting>8</Crafting>
      </skillRequirements>
